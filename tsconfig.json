{
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@fixtures/*": ["fixtures/*"],
      "@pages/*": ["pages/*"],
      "@utils/*": ["utils/*"],
      "@validations/*": ["validations/*"],

    },
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "target": "ESNext",
    "module": "CommonJS",
    "types": ["node", "playwright"]
  },
  "include": ["src/**/*.ts"]
}